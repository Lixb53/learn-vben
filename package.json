{
  "name": "learn-vben",
  "version": "0.0.0",
  "scripts": {
    "bootstrap": "yarn install",
    "dev": "npx --max_old_space_size=4096 vite",
    "serve": "npx --max_old_space_size=4096 vite preview",
    "build": "cross-env NODE_ENV=production vite build && esno ./build/script/postBuild.ts",
    "build:no-cache": "yarn clean:cache && yarn build",
    "report": "cross-env REPORT=true npm run build",
    "gen:icon": "esno ./build/generate/icon/index.ts",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && npm run bootstrap",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:ls-lint": "ls-lint",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "install:husky": "is-ci || husky install",
    "postinstall": "npm run install:husky",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "@ant-design/colors": "^6.0.0",
    "@iconify/iconify": "^2.0.4",
    "@vueuse/core": "^6.0.0",
    "ant-design-vue": "^2.2.6",
    "autoclean": "^1.0.2",
    "axios": "^0.21.1",
    "crypto-js": "^4.1.1",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "moment": "^2.29.1",
    "pinia": "^2.0.0-rc.6",
    "qs": "^6.10.1",
    "vue": "^3.0.5",
    "vue-i18n": "^9.1.7",
    "vue-router": "^4.0.11",
    "vue-types": "^4.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@iconify/json": "^1.1.390",
    "@ls-lint/ls-lint": "^1.10.0",
    "@purge-icons/generated": "^0.7.0",
    "@types/fs-extra": "^9.0.12",
    "@types/inquirer": "^7.3.3",
    "@types/node": "^16.6.1",
    "@types/rollup-plugin-visualizer": "^4.2.1",
    "@types/vue-router": "^2.0.0",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "@vitejs/plugin-legacy": "^1.5.1",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.3.1",
    "chalk": "^4.1.2",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.16.0",
    "esno": "^0.9.1",
    "fs-extra": "^10.0.0",
    "global": "^4.4.0",
    "http-server": "^13.0.1",
    "husky": "^7.0.1",
    "inquirer": "^8.1.2",
    "is-ci": "^3.0.0",
    "less": "^4.1.1",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "rollup-plugin-visualizer": "^5.5.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.3.2",
    "vite": "^2.4.4",
    "vite-plugin-compression": "^0.3.3",
    "vite-plugin-html": "^2.0.7",
    "vite-plugin-mock": "^2.9.4",
    "vite-plugin-purge-icons": "^0.7.0",
    "vite-plugin-pwa": "^0.11.0",
    "vite-plugin-style-import": "^1.1.1",
    "vite-plugin-svg-icons": "^1.0.4",
    "vite-plugin-theme": "^0.8.1",
    "vite-plugin-windicss": "^1.2.7",
    "vue-eslint-parser": "^7.10.0",
    "vue-tsc": "^0.2.2",
    "yargs": "^17.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
